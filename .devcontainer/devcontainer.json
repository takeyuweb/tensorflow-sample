{
    "name": "tensorflow-sample",
    "dockerComposeFile": [
        "../docker-compose.yml"
    ],
    "service": "tensorflow",
    "workspaceFolder": "/tensorflow-sample",
    "extensions": [
        "ms-python.python"
    ],
    "settings": {
        "editor.tabSize": 4,
        "editor.formatOnSave": true,
        "terminal.integrated.defaultProfile.linux": "bash",
        "terminal.integrated.profiles.linux": {
            "bash": {
                "path": "bash",
                "args": [
                    "-l"
                ]
            }
        }
    }
}